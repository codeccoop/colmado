{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-transform-runtime", ["module-resolver", { "root": ["./"] }]],
  "env": {
    "production": {
      "plugins": ["@babel/plugin-transform-react-constant-elements"]
    }
  }
}
